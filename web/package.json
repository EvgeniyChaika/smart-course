  "scripts": {
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --config webpack.config.js --inline --progress --colors --watch --hot --content-base ./",
    "server:prod": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --progress --profile --display-reasons --colors --display-error-details --display-cached",
    "prestart": "babel-node tools/startMessage.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "build": "npm run server:prod",
    "start": "npm-run-all --parallel server:dev lint:watch"
  },
